@{
    ViewData["Title"] = "BREAKDOWN";
}

<input type="hidden" id="reporturl" value="@ViewBag.ReportUrl" />


<div class="text-center">
    <h2 class="display-5">BREAKDOWN</h2>
</div>

<div class="row">
    <iframe 
        id="reportFrame"
        style="width: 100%; height:1000px">
    </iframe>
</div>



@section Scripts{
<script>

    let reportUrl = document.getElementById("reporturl").value;
    let urlReport = reportUrl + "BreakdownReport&rs:Command=Render";
    document.getElementById("reportFrame").setAttribute("src", urlReport);

</script>
}